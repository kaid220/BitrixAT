<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="INFO">
    <Appenders>
        <Console name="stdout">
            <PatternLayout charset="UTF-8" pattern="%d [%c{1}] %-5level- %msg%n%throwable"/>
        </Console>
    </Appenders>
    <Loggers>
     <Root level="ALL">
         <AppenderRef ref="stdout"/>
    </Root>
    </Loggers>
</Configuration>